<html>
	<head>
		<title>Axe Data Mapper</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<!--[if lte IE 8]><script src="/assets/js/ie/html5shiv.js"></script><![endif]-->
		<link rel="stylesheet" href="/assets/css/main.css" />
		<link rel="shortcut icon" type="image/png" href="/assets/images/favicon.png">
		<!--[if lte IE 8]><link rel="stylesheet" href="/assets/css/ie8.css" /><![endif]-->
		<!--[if lte IE 9]><link rel="stylesheet" href="/assets/css/ie9.css" /><![endif]-->
	</head>
	<body id="top">

		<!-- Header -->
      <header id="header" style="width: 100%; height: 100%; position: fixed;">
        <div class="content align-center">
          <% if (success) { %>
  					<h1><a href="#">Hi <%= name %>, Your email has been verified!</a></h1>
						<p>Go back to your application and change your password.</p>
          <% } %>
          <% if (!success) { %>
  					<h1><%= message %></h1>
  					<p>
              Please go back to your application and request <br>
              another email verification token.<br><br>
              We appologize for the inconvenience. <br>
            </p>
            <span class="icon fa-frown-o" style="font-size: 5em"><span class="label">sad</span></span>
          <% } %>
        </div>
      </header>

		<!-- Scripts -->
			<script src="/assets/js/jquery.min.js"></script>
			<script src="/assets/js/jquery.scrolly.min.js"></script>
			<script src="/assets/js/skel.min.js"></script>
			<script src="/assets/js/util.js"></script>
			<!--[if lte IE 8]><script src="/assets/js/ie/respond.min.js"></script><![endif]-->
			<script src="/assets/js/main.js"></script>

	</body>
</html>
